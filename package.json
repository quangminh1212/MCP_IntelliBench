{
  "name": "@mcp/intellibench",
  "version": "1.0.0",
  "description": "A comprehensive MCP (Model Context Protocol) Server for evaluating AI Coding Intelligence",
  "author": "MCP IntelliBench Team",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai",
    "benchmark",
    "coding",
    "evaluation",
    "llm",
    "artificial-intelligence"
  ],
  "homepage": "https://github.com/quangminh1212/MCP_IntelliBench",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quangminh1212/MCP_IntelliBench.git"
  },
  "bugs": {
    "url": "https://github.com/quangminh1212/MCP_IntelliBench/issues"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.0",
  "main": "./dist/server/index.js",
  "module": "./dist/server/index.js",
  "types": "./dist/server/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js"
    },
    "./dashboard": {
      "types": "./dist/dashboard/index.d.ts",
      "import": "./dist/dashboard/index.js"
    }
  },
  "bin": {
    "intellibench": "./dist/cli/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsx watch src/server/index.ts",
    "dev:dashboard": "vite dev",
    "build": "pnpm run build:server && pnpm run build:dashboard",
    "build:server": "tsc -p tsconfig.server.json",
    "build:dashboard": "vite build",
    "start": "node dist/server/index.js",
    "start:server": "node dist/server/index.js",
    "start:dashboard": "vite preview --port 8080",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --dir tests/unit",
    "test:integration": "vitest run --dir tests/integration",
    "test:e2e": "vitest run --dir tests/e2e",
    "test:quick": "tsx scripts/quick-test.ts",
    "benchmark": "tsx scripts/run-benchmark.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist coverage .cache",
    "prepare": "husky"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "better-sqlite3": "^11.7.0",
    "chalk": "^5.3.0",
    "commander": "^13.0.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "nanoid": "^5.0.9",
    "ora": "^8.1.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vercel/node": "^5.5.16",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.6",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "zod": "^3.25.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
